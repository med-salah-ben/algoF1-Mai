PROCEDURE Card_sort(var tab : ARRAY_OF INTEGER)
VAR
    i, j , key :INTEGER;
BEGIN
    FOR i FROM 1 TO tab.length -1 STEP 1  DO
        key := tab[i];
        j := i - 1;
        WHILE (j>=0 && tab[j]>key) DO
            tab[j+1] = tab[j];
            j= j --
        END_WHILE
        tab[j+1] = key
    END_FOR
END